datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

model Box {
  id      String  @id
  code    String  @unique
  name    String
  address String
  location Unsupported("geography(Point,4326)")
  status  String?
} 